{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "expand": {
      "type": "string",
      "description": "Expand options that include additional issue details in the response.",
      "readOnly": true,
      "xml": {
        "attribute": true
      }
    },
    "id": {
      "type": "string",
      "description": "The ID of the issue.",
      "readOnly": true
    },
    "self": {
      "type": "string",
      "description": "The URL of the issue details.",
      "format": "uri",
      "readOnly": true
    },
    "key": {
      "type": "string",
      "description": "The key of the issue.",
      "readOnly": true
    },
    "renderedFields": {
      "type": "object",
      "additionalProperties": true,
      "description": "The rendered value of each field present on the issue.",
      "readOnly": true
    },
    "properties": {
      "type": "object",
      "additionalProperties": true,
      "description": "Details of the issue properties identified in the request.",
      "readOnly": true
    },
    "names": {
      "type": "object",
      "additionalProperties": true,
      "description": "The ID and name of each field present on the issue.",
      "readOnly": true
    },
    "schema": {
      "type": "object",
      "description": "The schema describing each field present on the issue.",
      "readOnly": true
    },
    "transitions": {
      "type": "array",
      "description": "The transitions that can be performed on the issue.",
      "readOnly": true
    },
    "operations": {
      "description": "The operations that can be performed on the issue.",
      "readOnly": true
    },
    "editmeta": {
      "description": "The metadata for the fields on the issue that can be amended.",
      "readOnly": true
    },
    "changelog": {
      "type": "object",
      "description": "Details of changelogs associated with the issue.",
      "properties": {
        "histories": {
          "type": "array",
          "description": "The list of changelogs.",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "author": {
                "properties": {
                  "self": {
                    "type": "string"
                  },
                  "accountId": {
                    "type": "string"
                  },
                  "emailAddress": {
                    "type": "string"
                  },
                  "displayName": {
                    "type": "string"
                  },
                  "active": {
                    "type": "boolean"
                  },
                  "timeZone": {
                    "type": "string"
                  },
                  "accountType": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              },
              "created": {
                "type": "string",
                "format": "date"
              },
              "items": {
                "type": "array",
                "items": {
                  "properties": {
                    "field": {
                      "type": "string"
                    },
                    "fieldType": {
                      "type": "string"
                    },
                    "fieldId": {
                      "type": "string"
                    },
                    "from": {
                      "type": "string"
                    },
                    "fromString": {
                      "type": "string"
                    },
                    "to": {
                      "type": "string"
                    },
                    "toString": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": true
                }
              }
            },
            "additionalProperties": true
          }
        },
        "maxResults": {
          "type": "number",
          "format": "int32",
          "description": "The maximum number of results that could be on the page."
        },
        "startAt": {
          "type": "number",
          "format": "int32",
          "description": "The index of the first item returned on the page."
        },
        "total": {
          "type": "number",
          "format": "int32",
          "description": "The number of results on the page."
        }
      },
      "additionalProperties": true
    },
    "versionedRepresentations": {
      "type": "object",
      "additionalProperties": true,
      "description": "The versions of each field on the issue.",
      "readOnly": true
    },
    "fieldsToInclude": {
      "type": "object"
    },
    "fields": {
      "type": "object",
      "properties": {
        "created": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time",
          "description": "This field is not shown in schema / swagger, but exists in records and we use it as cursor fiekd. Updated may be absent. Added to solve the #4341"
        },
        "updated": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time",
          "description": "This field is not shown in schema / swagger, but exists in records and we use it as cursor fiekd. Updated may be absent. Added to solve the #4341"
        }
      },
      "additionalProperties": true
    },
    "projectId": {
      "type": "string",
      "description": "The ID of the project containing the issue.",
      "readOnly": true
    },
    "projectKey": {
      "type": "string",
      "description": "The key of the project containing the issue.",
      "readOnly": true
    },
    "created": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "description": "This field transformed from fields attr",
      "readOnly": true
    },
    "updated": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "description": "This field transformed from fields attr",
      "readOnly": true
    }
  },
  "additionalProperties": true
}